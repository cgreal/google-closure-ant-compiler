<?xml version="1.0"?>
<project basedir="." default="compile">

  <taskdef name="jscomp" classname="com.google.javascript.jscomp.ant.CompileTask" classpath="compiler.jar"/>

  <target name="compile">
    
    <jscomp compilationLevel="simple" warning="verbose" debug="false" output="bigfile.js">

      <sources dir="${basedir}">
        <file name="bigfile.js"/>
      </sources>

    </jscomp>
    
  </target>

</project>